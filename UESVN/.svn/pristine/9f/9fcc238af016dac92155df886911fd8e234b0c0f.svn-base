<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>zustruct</Title>
			<Shortcut>zustruct</Shortcut>
			<Description>UE 结构体构建</Description>
			<Author>Zery</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<ToolTip>结构体名称</ToolTip>
					<Default>FStruct</Default>
				</Literal>
			</Declarations>
			<Code Language="cpp"><![CDATA[
				USTRUCT(BlueprintType)
				struct $name$
	{
		GENERATED_USTRUCT_BODY();
		$selected$$end$
	};]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
